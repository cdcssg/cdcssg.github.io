---
layout: 
dateformat: "%Y%m%dT%H%M00"
---

BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//hacksw/handcal//NONSGML v1.0//EN
{% for event in site.events %}
BEGIN:VEVENT
UID:{{ event.url }}
DTSTAMP:{{ site.time | date: page.dateformat }}
DTSTART:{{ event.start | date: page.dateformat }}
DTEND:{{ event.end | date: page.dateformat }}
SUMMARY:{{ event.title }}
END:VEVENT
{% endfor %}
END:VCALENDAR